    }

    .product-card {
      border: none;
      text-align: center;
    }

    .product-card img {
      width: 100%;
      object-fit: cover;
    }

    .product-card .colors span {
      display: inline-block;
      width: 12px;
      height: 12px;
      border-radius: 50%;
      margin: 0 2px;
      border: 1px solid #ccc;
